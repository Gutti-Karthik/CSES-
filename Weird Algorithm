#include<bits/stdc++.h>
using namespace std;
#define int long long
 
void solve(){
int n; cin >> n;
 
cout << n << " ";
 
while(n!=1){
 
if(n%2==0){
    n = n/2;
    cout << n << " ";
 }
else{
    n = n*3+1;
    cout << n << " ";
  }
 
}
cout << "\n";
 
}
 
signed main(){
    
    ios_base::sync_with_stdio(0);
    cin.tie(0);cout.tie(0);
   // int _t; cin>>_t; while(_t--)
        solve();
}
